{
  "name": "ofitec-mvp",
  "dockerComposeFile": ["../docker-compose.yml"],
  "service": "backend",
  "workspaceFolder": "/workspaces/pptosofitec.ai",
  "runServices": ["db", "redis", "backend", "frontend"],
  "features": {},
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance",
        "ms-azuretools.vscode-docker",
        "esbenp.prettier-vscode",
        "ms-python.black-formatter",
        "eamodio.gitlens"
      ]
    }
  },
  "forwardPorts": [3000, 5555],
  "postCreateCommand": "pip install -r backend/requirements.txt"
}
