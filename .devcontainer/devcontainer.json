{
  "name": "ofitec-mvp",
  "dockerComposeFile": ["../docker-compose.yml"],
  "service": "backend",
  "workspaceFolder": "/workspaces/pptosofitec.ai",
  "runServices": ["db", "redis", "backend"],
  "remoteUser": "appuser",
  "overrideCommand": false,
  "features": {},
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance",
        "ms-azuretools.vscode-docker",
        "esbenp.prettier-vscode",
        "ms-python.black-formatter",
        "eamodio.gitlens"
      ]
    }
  },
  "forwardPorts": [5555, 3001],
  "postCreateCommand": "pip install --no-cache-dir -r backend/requirements.txt || true"
}
